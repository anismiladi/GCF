<script>
    //====================================
    // Fullcalender scripts
    //====================================
    var data =
        [
            {
                title: 'test',
                start: '2018-06-06',
                end: '2018-06-06',
                url: '#'
            },
            {
                title: 'test2',
                start: '2018-06-06',
                end: '2018-06-06',
                url: '#'
            },
        ];
    $.ajax({
        url: '{{ path('gcf_front_event_page') }}',
        type: 'POST',
        data: { 'test' : 'OK' },
        dataType: 'json',
        contentType: 'application/json',
        success: function (result) {
            //console.log(result);
            for ( i = 0; i < result.length ; i++){
                var url = '{{ path("gcf_front_single_event_page",{"slug": 'slug', "id": 'id'}) }}';
                url = url.replace('id', result[i].id);
                url = url.replace('slug', result[i].nom.toLowerCase().trim().replace(" ", "-"));
                data.push(
                    {
                        title: result[i].nom,
                        start: result[i].debut.date.substr(0,10),
                        end: result[i].fin.date.substr(0,10),
                        url: url
                    },
                )
            }

            var calendar = $('#calendar_test');

            var bgEvent = {
                id: -1,
                start: null,
                end: null,
                rendering: 'background',
                backgroundColor: 'blue'
            };
            var today = new Date();
            var dd = today.getDate();
            var mm = today.getMonth()+1; //January is 0!
            var yyyy = today.getFullYear();

            calendar.fullCalendar({
                locale: '{{ app.request.locale }}',
                header: {
                    left: 'prev,next today',
                    center: '',
                    right: 'title '
                },
                eventMouseover: function(calEvent, jsEvent) {
                    var tooltip = '<div class="tooltipevent">' + calEvent.title + '</div>';
                    var $tooltip = $(tooltip).appendTo('body');
                    $(this).mouseover(function(e) {
                        $(this).css('z-index', 10000);
                        $tooltip.fadeIn('500');
                        $tooltip.fadeTo('10', 1.9);
                    }).mousemove(function(e) {
                        $tooltip.css('top', e.pageY - 20);
                        $tooltip.css('left', e.pageX - 220);
                    });
                },

                eventMouseout: function(calEvent, jsEvent) {
                    $(this).css('z-index', 8);
                    $('.tooltipevent').remove();
                },
                eventClick:  function(event, jsEvent, view) {
                    //$("#calendar_test").tooltip({ modal: true, title: event.title });
                    $(this).css('border-color', '#22AA86');
                },
                selectable: true,
                defaultDate: yyyy+'-'+mm+'-'+dd,
                editable: true,
                eventLimit: true, // allow "more" link when too many events
                events: data
            });
            //console.log(data)
        },
        error: function (error) {
            //console.log( ( error) )
        }

    });
</script>

{% endif %}
{% block javascripts %}{% endblock %}
</body>
</html>
