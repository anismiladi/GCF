<script>

    //====================================
    // saving gb post
    //====================================
    $('#gb_posts_form').submit(function(e) {

        e.preventDefault();
        $.ajax({
            url: '{{ path('gcf_front_save_gb_post_page') }}',
            data: $(this).serialize(),
            type: 'POST',
            dataType: 'json',
            success: function (result) {
               console.log(result);
                var responseToAppend = '';

                responseToAppend =  '<div class="notif_pub_success">';
                responseToAppend += '<p>';
                responseToAppend += 'Votre article est pris en compte';
                responseToAppend += '</p>';
                responseToAppend += '</div>';

                $('#notif_pub').empty();
                $('#notif_pub').addClass('success');
                $('#notif_pub').append(responseToAppend);

                $('#gb_name').val('');
                $('#gb_email').val('');
                $('#gb_article_title').val('');
                $('#cke_gb_article_content').val('');
                $('#gb_article_project').val('');

                return false;
            },
            error: function (error) {
                console.log(error);

                var responseToAppend = '';
                responseToAppend =  '<div class="notif_pub_error">';
                responseToAppend += '<p>';
                responseToAppend += ' S"il vous plaiz essayer plus tard et si l"erreur persist contactez-nous';
                responseToAppend += '</p>';
                responseToAppend += '</div>';

                $('#notif_pub').empty();
                $('#notif_pub').addClass('error');
                $('#notif_pub').append(responseToAppend);

                return false;
            }
        });
        return false;

    });
</script>

