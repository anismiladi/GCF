                                    <a href="{% if autrespublication.titre!= '' %} {{ path('gcf_front_single_publications_page',{'slug': autrespublication.titre|lower|replace({' ': '-'}), 'id': autrespublication.id }) }} {% endif %}">
                                        <img class="img-fluid" src="{% if autrespublication.featuredImage!= '' %}{{ autrespublication.featuredImage|replace({'/app_dev.php': ''}) }}{% else %}{{ asset('assets/images/empty-image.png') }}{% endif %}" title="{{ autrespublication.titre }}" style="max-width: 100%;max-height: 100%; min-height: 121px; object-fit: cover;"/>
                                    </a>
                                </div>
                                <div class="col-md-8 col-lg-8 col-xs-8">
                                    <h3 class="name"><a href="{% if autrespublication.titre!= '' %} {{ path('gcf_front_single_publications_page',{'slug': autrespublication.titre|lower|replace({' ': '-'}), 'id': autrespublication.id }) }} {% endif %}">{{ autrespublication.titre }}</a></h3>
                                    <p style="margin:0"><i class="fa fa-clock-o" aria-hidden="true"></i>{{ autrespublication.createdAt|date('d-m-Y') }} </p>
                                    <p class="description" style="font-size: 13px;">{{ autrespublication.contenu|slice(0, 117)|striptags  }} ...</p>
                                </div>
                            </div>
                        </div>
                        {% endif %}
                    {% endfor %}

                    {% for gbpublication in gbpublications %}
                        {% if gbpublication.titre != "" and  gbpublication.etatPub == 'Public' %}
                            <div class="col-sm-6 item filter_display gb" style="background: white;border: solid 2px #f7f7f7;">
                                <div class="row">
                                    <div class="col-md-4 col-lg-4 col-xs-4" style="padding: 0;">
                                        <a  href="{% if gbpublication.titre!= '' %} {{ path('gcf_front_single_publications_page',{'slug': gbpublication.titre|lower|replace({' ': '-'}), 'id': gbpublication.id }) }} {% endif %}">
                                            <img class="img-fluid" src="{% if gbpublication.featuredImage!= '' %}{{ gbpublication.featuredImage|replace({'/app_dev.php': ''}) }}{% else %}{{ asset('assets/images/empty-image.png') }}{% endif %}" title="{{ gbpublication.titre }}" style="max-width: 100%;max-height: 100%; min-height: 121px; object-fit: cover;"/>
                                        </a>
                                    </div>
                                    <div class="col-md-8 col-lg-8 col-xs-8">
                                        <h3 class="name"><a href="{% if gbpublication.titre!= '' %} {{ path('gcf_front_single_publications_page',{'slug': gbpublication.titre|lower|replace({' ': '-'}), 'id': gbpublication.id }) }} {% endif %}">{{ gbpublication.titre }}</a></h3>
                                        <p style="margin:0"><i class="fa fa-clock-o" aria-hidden="true"></i>{{ gbpublication.createdAt|date('d-m-Y') }} </p>
                                        <p class="description" style="font-size: 13px;">{{ gbpublication.contenu|slice(0, 117)|striptags  }} ...</p>
                                    </div>
                                </div>
                            </div>
                        {% endif %}
                    {% endfor %}
                </div>
                <div class="row read_more_container col-sm-12">
                    <a href="{{ path('gcf_front_publicationspage') }}">
                        <button class="button_see_more_lg">
                            Voir Plus
                        </button>
                    </a>
                </div>
            </div>
        </div>
    </div>

<div class="index__overlay" onclick="closeVideoPopup()"></div>
<div class="video__home__popup">
    <div class="video__container">
        <a class="close__video__popup" style="position: absolute; right: 20px ; top: 0; font-size: 35px; color:gray" onclick="closeVideoPopup()">&times;</a>
        <iframe width="100%" height="100%" src=" " frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
    </div>
</div>
{% endblock %}

{% block javascripts %}
    <script>
        $('.play__youtube__video').click( function () {
            var videoUrl = $(this).attr('data-url-youtube');
            videoUrl = videoUrl.replace(/(.*)v=([^&]*)&?(.*)/, "https://www.youtube.com/embed/$2");

            $('.video__container iframe').attr('src', videoUrl);

            $('.index__overlay').fadeIn("slow");
            $('.video__home__popup').fadeIn( );
        } );
        function closeVideoPopup() {
            $('.index__overlay').fadeOut("slow");
            $('.video__home__popup').fadeOut();
            $('.video__container iframe').attr('src', '');
        }
    </script>

    <script>
        $('.slider_projects').slick({
            dots: false,
            infinite: false,
            slidesToShow: 5,
            slidesToScroll: 1,

            touchMove: false,
            autoplay: true,
            autoplaySpeed: 5000,
            responsive: [
                {
                    breakpoint: 1023,
                    settings: {
                        speed: 10,
                        infinite: false,
                        slidesToShow: 3,
                        slidesToScroll: 1,
                        arrows: false,
                        touchMove: true,
                        autoplay:true,
                        autoplaySpeed:5000

                    }
                },
            ]
        });
    </script>
    <script>
        $(document).ready(function(){

            $(".filter-button").click(function(){
                $(".filter-button").removeClass("active");
                $(this).addClass("active");
                var value = $(this).attr('data-filter');

                if(value == "all")
                {
                    //$('.filter').removeClass('hidden');
                    $('.filter_display').show('3000');
                }
                else
                {
//            $('.filter[filter-item="'+value+'"]').removeClass('hidden');
//            $(".filter").not('.filter[filter-item="'+value+'"]').addClass('hidden');
                    $(".filter_display").not('.'+value).hide('3000');
                    $('.filter_display').filter('.'+value).show('3000');

                }
            });

        });
    </script>


    <script>
        jQuery('#technique_tab').click(function () {

            jQuery('#technique_tab_container').css('display', 'block');
            jQuery('#legislatives_tab_container').css('display', 'none');
            jQuery('#technique_tab').addClass('active');
            jQuery('#legislatives_tab').removeClass('active');

        });

        jQuery('#legislatives_tab').click(function () {

            jQuery('#legislatives_tab_container').css('display', 'block');
            jQuery('#technique_tab_container').css('display', 'none');
            jQuery('#legislatives_tab').addClass('active');
            jQuery('#technique_tab').removeClass('active');

        });
    </script>

    <script>
        {% for key, secteur in secteurs %}
        jQuery('#{{ secteur.nom|replace({' ': '_'}) }}_tab').click(function () {
            jQuery('._tab_container').hide();
            jQuery('#{{ secteur.nom|replace({' ': '_'}) }}_tab_container').show();
            jQuery('.header__tabs').removeClass('active');
            jQuery('#{{ secteur.nom|replace({' ': '_'}) }}_tab').addClass('active');
        });
        {% endfor %}
    </script>
{% endblock %}