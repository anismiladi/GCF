{% extends '@GCFFront/base.html.twig' %}

{% block title %} {% endblock %}

{% block body %}
    {{ dump(evenement) }}







    <div class="single__event__page__container col-md-12">
        <div class="single__event__page__header">
            <img src="{{ evenement.photoCouverture }}" alt="{{ evenement.nom }}">
            <h1>{{ evenement.nom }}</h1>
            <p>{{ evenement.debut|date("d-m-Y") }}</p>
            <p>{{ evenement.debut|date("g:ia")}} - {{ evenement.fin|date("g:ia")}}</p>
            <p>{{ evenement.lieu }}</p>
        </div>
        <div class="single__event__page__body">



            {{ evenement.description }}
        </div>

    </div>
{% endblock %}



